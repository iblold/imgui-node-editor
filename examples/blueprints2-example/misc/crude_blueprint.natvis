<?xml version="1.0" encoding="utf-8"?>
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">

    <Type Name="crude_blueprint::Node">
        <DisplayString>{{ {m_Id}: {m_Name} }} Node</DisplayString>
    </Type>

    <Type Name="crude_blueprint::Pin">
        <AlternativeType Name="crude_blueprint::AnyPin"/>
        <AlternativeType Name="crude_blueprint::FlowPin"/>
        <AlternativeType Name="crude_blueprint::BoolPin"/>
        <AlternativeType Name="crude_blueprint::Int32Pin"/>
        <AlternativeType Name="crude_blueprint::FloatPin"/>
        <AlternativeType Name="crude_blueprint::StringPin"/>
        <DisplayString Condition="m_Link" Optional="true">{{ {m_Id}: {m_Type,en} ({(*m_InnerPin).m_Type,en}), {m_Name} }} Pin -&gt; {*m_Link}</DisplayString>
        <DisplayString Condition="m_Link">{{ {m_Id}: {m_Type,en}, {m_Name} }} Pin -&gt; {*m_Link}</DisplayString>
        <DisplayString Optional="true">{{ {m_Id}: {m_Type,en}, ({(*m_InnerPin).m_Type,en}), {m_Name} }} Pin</DisplayString>
        <DisplayString>{{ {m_Id}: {m_Type,en}, {m_Name} }} Pin</DisplayString>
        <Expand>
            <Item Name="Id">m_Id</Item>
            <Item Name="Name">m_Name</Item>
            <Item Name="Node">*m_Node</Item>
            <Item Name="Type">m_Type</Item>
            <Item Name="[Local Value]" Optional="true">m_Value</Item>
            <Item Name="Link" Condition="m_Link">*m_Link</Item>
            <Item Name="[Type]" Optional="true">GetValueType()</Item>
            <Item Name="[Value]" Optional="true">GetValue()</Item>
            <Item Name="[Inner]" Optional="true">*m_InnerPin</Item>
        </Expand>
    </Type>

    <Type Name="crude_blueprint::PinValue">
        <DisplayString>{{ Type={(crude_blueprint::PinType)m_Value.type_index,en}, Value={m_Value,view(value)} }}</DisplayString>
        <Expand>
            <Item Name="Type">(crude_blueprint::PinType)m_Value.type_index,en</Item>
            <Item Name="Value">m_Value,view(value)</Item>
        </Expand>
    </Type>

</AutoVisualizer>
